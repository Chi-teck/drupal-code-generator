id: {{ plugin_id }}
label: {{ plugin_label }}
migration_tags:
  - Drupal 7
source:
  plugin: {{ source_plugin }}
process:
  # See https://www.drupal.org/node/2129651 on how to define
  # process map for the migration.
{% for field in fields %}
  {{ field }}: {{ field }}
{% endfor %}
destination:
  plugin: {{ destination_plugin }}
