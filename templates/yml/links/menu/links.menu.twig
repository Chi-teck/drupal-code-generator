# Simple link.
{{ machine_name }}.node_add:
  title: Add content
  description: Add new content.
  menu_name: main
  route_name: node.add_page
  weight: 10

# Child link.
{{ machine_name }}.node_add_article:
  title: Add article
  menu_name: main
  parent: {{ machine_name }}.node_add
  route_name: node.add
  route_parameters:
    node_type: article
  weight: 20

