{
  "chmod": "0755",
  "finder": [
    {
      "name": "*",
      "in": "src",
      "exclude": ["Box"]
    },
    {
      "name": "*",
      "in": "templates"
    },
    {
      "name": "*.php",
      "in": [
        "vendor"
      ],
      "exclude": [
        "Tests",
        "test"
      ]
    }
  ],
  "git-version": "git-version",
  "main": "bin/dcg",
  "output": "dcg.phar",
  "bootstrap": "src/Box/PhpCompactor.php",
  "compactors": [
    "DrupalCodeGenerator\\Box\\PhpCompactor"
  ],
  "stub": true
}
