services:
  {{ machine_name }}.example:
    class: Drupal\{{ machine_name }}\Example
    arguments: ['@entity.query', '@entity.manager']
  {{ machine_name }}.middleware:
    class: Drupal\{{ machine_name }}\{{ name }}Middleware
    tags:
      - { name: http_middleware, priority: 10 }
  {{ machine_name }}.event_subscriber:
      class: Drupal\{{ machine_name }}\EventSubscriber\{{ name }}Subscriber
      arguments: ['@current_user']
      tags:
        - { name: event_subscriber }
